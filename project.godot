config_version=5

[application]

config/name="SageRage"
run/main_scene="res://src/main_menu.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
boot_splash/image="res://assets/splash.png"
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

